import java.io.*;
import java.util.*;
class Main
{
    public static void main(String args[])
    {
        int n;
        Scanner in = new Scanner(System.in);
        System.out.println("Enter the n value: ");
        n=in.nextInt();
        int sum=0;
        for(int i=1;i<=n;i++)
        {
            if(i%2==0)
             System.out.println("Even Numbers: "+i);
        }
        
    }
}
